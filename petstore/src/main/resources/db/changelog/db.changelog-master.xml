<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:pro="http://www.liquibase.org/xml/ns/pro"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.9.xsd">
	<changeSet id="202010211812" author="Milos Curuvija">
		<createTable tableName="locations">
			<column name="id" type="bigint">
				<constraints primaryKey="true" primaryKeyName="id" />
			</column>
			<column name="name" type="varchar(250)">
				<constraints unique="false" />
			</column>
			<column name="fully_paid" type="boolean" defaultValueBoolean="false"></column>
		</createTable>
		<addAutoIncrement tableName="locations" columnName="id" columnDataType="bigint" startWith="1" incrementBy="1" />
		<createSequence sequenceName="hibernate_sequence" incrementBy="1" startValue="1" />
		<addForeignKeyConstraint baseTableName="item" baseColumnNames="house_id" constraintName="item_house_id_fk" referencedTableName="house" referencedColumnNames="id" />
	</changeSet>
</databaseChangeLog>